{
  "name": "accessibility-ecomm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "commit": "git-cz"
  },
  "dependencies": {
    "next": "10.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "commitizen": "^4.2.4",
    "eslint": "^6.8.0",
    "git-cz": "^4.7.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged --allow-empty",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "browserslist": [
      "last 2 versions",
      "Android >= 4",
      "IE >= 11"
    ]
  }
}
